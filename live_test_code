class Media{

     play(){
          print("Playing media...");
     }
}


class Song extends Media{
     String? artist;

     @override
     play(){
          print("Playing song by $artist...");
     }



}

void main(){

     Media media = Media();
     media.play();



     Song song = Song();
     song.artist = "Sumon";
     song.play();
}

